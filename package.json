{
  "name": "detalles-jocelyn-ecommerce",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^12.20 || >= 14.13"
  },
  "scripts": {
    "run:server": "ts-node ./src/index.ts",
    "run:worker": "ts-node ./src/index-worker.ts",
    "start-dev": "concurrently yarn:run:*",
    "start": "node dist/src/index-worker.js & node dist/src/index.js",
    "start-worker": "node dist/index-worker.js",
    "build": "tsc",
    "migration:generate": "ts-node migration generate",
    "migration:run": "ts-node migration run",
    "migration:revert": "ts-node migration revert",
    "deploy:local": "./dockerize-local.sh",
    "deploy:prod": "./deploy-prod.sh",
    "deploy:dev": "./deploy-dev.sh"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.7.2",
    "@types/nodemailer": "^6.4.1",
    "@types/tmp": "^0.2.0",
    "@vendure/admin-ui-plugin": "1.7.1",
    "@vendure/asset-server-plugin": "1.7.1",
    "@vendure/core": "1.7.1",
    "@vendure/email-plugin": "1.7.1",
    "dotenv": "^8.2.0",
    "exact": "^0.8.0",
    "pg": "8.5.1",
    "tmp": "^0.2.1",
    "typescript": "4.0.3"
  },
  "devDependencies": {
    "concurrently": "5.3.0",
    "ts-node": "9.1.1"
  }
}
